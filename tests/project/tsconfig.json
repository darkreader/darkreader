{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "target": "ESNext",
        "module": "Node16",
        "lib": [
            "ESNext",
            "Dom"
        ],
        "types": [
            "node",
            "jest",
            "jest-extended"
        ],
        "esModuleInterop": true,
        "moduleResolution": "Node16",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "inlineSourceMap": true,
        "noEmit": true,
        "allowJs": true,
        "allowImportingTsExtensions": true
    }
}
