{
    "author": "Alexander Shutov",
    "background": {
        "page": "background/background.html",
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "19": "img/dr_active_19.png",
            "38": "img/dr_active_38.png"
        },
        "default_popup": "popup/popup.html",
        "default_title": "Dark Reader"
    },
    "commands": {
        "addSite": {
            "description": "Toggle current site",
            "suggested_key": {
                "default": "Alt+Shift+A"
            }
        },
        "toggle": {
            "description": "Toggle On/Off",
            "suggested_key": {
                "default": "Alt+Shift+D"
            }
        }
    },
    "description": "Dark theme for every website. Care your eyes, use Dark Reader for night and daily browsing.",
    "icons": {
        "16": "img/dr_16.png",
        "48": "img/dr_48.png",
        "128": "img/dr_128.png"
    },
    "Key": "",
    "manifest_version": 2,
    "name": "Dark Reader",
    "permissions": [
        "fontSettings",
        "storage",
        "tabs",
        "<all_urls>"
    ],
    "version": "3.5.1",
    "-ms-preload": {
        "backgroundScript": "backgroundScriptsAPIBridge.js",
        "contentScript": "contentScriptsAPIBridge.js"
    }
}