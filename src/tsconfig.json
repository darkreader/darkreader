{
    "compilerOptions": {
        "target": "es2019",
        "baseUrl": ".",
        "module": "es2015",
        "moduleResolution": "node",
        "lib": [
            "es2015",
            "es2017",
            "dom",
            "dom.iterable"
        ],
        "types": [
            "chrome",
            "offscreencanvas"
        ],
        "allowJs": true,
        "downlevelIteration": true,
        "noImplicitAny": true,
        "resolveJsonModule": true,
        "jsx": "react",
        "jsxFactory": "m",
    }
}
