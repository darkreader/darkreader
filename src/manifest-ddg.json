{
    "background": {
        "persistent": true,
        "page": "background/index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "inject/darkreader.js",
                "inject/index.js"
            ],
            "run_at": "document_start",
            "all_frames": true,
            "match_about_blank": true
        }
    ]
}
